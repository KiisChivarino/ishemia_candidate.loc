{% extends template.commonTemplatePath~'show.html.twig' %}
{% block show_content %}
    <li>
        <span class="title">{{template.items.show.content.name}}</span>
        <p>{{ entity.name }}</p>
    </li>
    <li>
        <span class="title">{{template.items.show.content.shortcode}}</span>
        <p>{{ entity.shortcode }}</p>
    </li>
{% endblock %}
